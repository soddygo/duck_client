{
  "tasks": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "imports": {
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "react-router-dom": "npm:react-router-dom@^6.28.0",
    "@tauri-apps/api": "npm:@tauri-apps/api@^2",
    "@tauri-apps/plugin-opener": "npm:@tauri-apps/plugin-opener@^2",
    "@tauri-apps/plugin-fs": "npm:@tauri-apps/plugin-fs@^2",
    "@tauri-apps/plugin-dialog": "npm:@tauri-apps/plugin-dialog@^2",
    "@tauri-apps/plugin-shell": "npm:@tauri-apps/plugin-shell@^2",
    "@tauri-apps/plugin-process": "npm:@tauri-apps/plugin-process@^2",
    "antd": "npm:antd@^5.22.2",
    "@ant-design/icons": "npm:@ant-design/icons@^5.5.1",
    "tailwindcss": "npm:tailwindcss@^3.4.0",
    "autoprefixer": "npm:autoprefixer@^10.4.16",
    "postcss": "npm:postcss@^8.4.32"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "allowJs": true,
    "lib": ["dom", "dom.iterable", "es2020"],
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules/",
    "dist/",
    "src-tauri/"
  ]
} 